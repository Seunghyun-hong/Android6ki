<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.thunder.myapplication">

    <!-- 권한 부여 -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CoffeeActivity"
            android:label="커피 주문"></activity>
        <activity
            android:name=".CountCounterActivity"
            android:label="농구점수"></activity>
        <activity
            android:name=".ActivityMoveActivity"
            android:label="버튼"></activity>
        <activity android:name=".TargetActivity" />
        <activity
            android:name=".LoginActivity"
            android:label="화면 전환"></activity>
        <activity android:name=".AdministerActivity" />
        <activity
            android:name=".ImplicitIntentActivity"
            android:label="인텐트 해보자"></activity>
        <activity
            android:name=".MyWebBrowserActivity"
            android:label="나만의 웹브라우저"></activity>
        <activity
            android:name=".SignUpExamActivity"
            android:label="SignUpExam"></activity>
        <activity
            android:name=".SignUpMessageActivity"
            android:label="가입정보확인" />
        <activity
            android:name=".adapterView.AdapterViewExamActivity"
            android:label="연락처"></activity>
        <activity
            android:name=".adapterviewexam.ColorfulAdapterActivity"
            android:label="알록달록 리스트뷰"></activity>
        <activity android:name=".adapterView.DetailAddressActivity" />
        <activity
            android:name=".LifeCycleActivity"
            android:label="액티비티 수명주기"></activity>
        <activity
            android:name=".fragment.basketball.BasketBallActivity"
            android:label="프래그먼트버전 농구"></activity>
        <activity android:name=".fragment.color.ColorActivity"></activity>
        <activity
            android:name=".fragment.exam.ColorSelectExamActivity"
            android:label="동적인 프래그먼트 과제"></activity>
        <activity
            android:name=".fragment.exam.suwonexam.ColorExamActivity"
            android:label="원장님이 만든 프래그먼트 붙이고 제거하고"></activity>
        <activity android:name=".ListViewActivityActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".fragment.exam.CommunicationExamActivity"
            android:label="인터페이스 콜백을 써보자">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>